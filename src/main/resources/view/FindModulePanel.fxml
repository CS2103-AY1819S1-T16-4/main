<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="moduleDescription" xmlns="http://javafx.com/javafx/10.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefWidth="186.0" spacing="5.0">
         <children>
            <Label fx:id="moduleCode" styleClass="moduleDescription_label_big" text="code" />
            <Label fx:id="moduleName" styleClass="moduleDescription_label_big" text="name" />
         </children>
      </HBox>
      <Label fx:id="moduleType" styleClass="moduleDescription_label_small" text="module type" />
      <Label fx:id="creditCount" styleClass="moduleDescription_label_small" text="credit count" />
      <HBox fillHeight="false" minWidth="340.0" prefWidth="340.0" spacing="1.0">
         <children>
            <Label fx:id="preclusion" minWidth="-Infinity" styleClass="moduleDescription_label_small" text="preclusions" />
            <FlowPane fx:id="preclusions" />
         </children>
      </HBox>
      <HBox fillHeight="false" minWidth="340.0" prefWidth="340.0" spacing="1.0">
         <children>
            <Label fx:id="prerequisite" minWidth="-Infinity" styleClass="moduleDescription_label_small" text="prerequisites" />
            <FlowPane fx:id="prerequisites" />
         </children>
      </HBox>
   </children>
</VBox>
